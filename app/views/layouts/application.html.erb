<!DOCTYPE html>
<html>
<head>
  <title>TeamStatus</title>
  <%= stylesheet_link_tag    'application' %>
  <%= javascript_include_tag 'application' %>
  <%= csrf_meta_tags %>
</head>
<body>
  <div class="container clearfix">
    <div class="link-section">
      <span class='layout_link'> << </span>
      <div class="layout_links">
        <ul>
          <% layout_links.each do |link_title, link_value| %>
            <li>
              <a href="<%=link_value%>">
                <%=link_title%>
              </a>
            </li>
          <% end %>
        </ul>
      </div>
    </div>
    <%=flash_html%>
    <%= yield %>
  </div>
</body>
</html>
